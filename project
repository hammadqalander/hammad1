//10
//program 1

// This program calculates the area of a circle given its radius.

// Define the radius of the circle
const radius: number = 5;

// Calculate the area of the circle using the formula A = Ï€ * r^2
const pi: number = 3.14159;
const area: number = pi * Math.pow(radius, 2);

// Print the calculated area
console.log(`The area of the circle with radius ${radius} is ${area}.`);


// program 2

// This program stores a person's name with whitespace, prints it with and without whitespace.

// Store the person's name with whitespace
const personNameWithWhitespace2: string = "\tJohn Doe\n";

// Print the name with whitespace
console.log("Name with Whitespace: '" + personNameWithWhitespace2 + "'");

// Strip the whitespace and print the cleaned name
const cleanedPersonName2: string = personNameWithWhitespace2.trim();
console.log("Cleaned Name: '" + cleanedPersonName2 + "'");


