//  21

// Define a type for a book
// Define a type for a book
type Book = {
    title: string;
    author: string;
    genre: string;
  };
  
  // Create an array of favorite books (represented as objects)
  const favoriteBooks: Book[] = [
    {
      title: "To Kill a Mockingbird",
      author: "Harper Lee",
      genre: "Fiction"
    },
    {
      title: "The Great Gatsby",
      author: "F. Scott Fitzgerald",
      genre: "Classic"
    },
    {
      title: "Harry Potter and the Sorcerer's Stone",
      author: "J.K. Rowling",
      genre: "Fantasy"
    }
  ];
  
  // Print information about the favorite books
  console.log("My Favorite Books:");
  for (const book of favoriteBooks) {
    console.log(`Title: ${book.title}`);
    console.log(`Author: ${book.author}`);
    console.log(`Genre: ${book.genre}`);
    console.log("------------------");
  }
  

