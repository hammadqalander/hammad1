// 14   guest list


// Create an array to store the names of people to invite
const invitees: string[] = ["Albert Einstein", "Marie Curie", "Leonardo da Vinci"];

// Loop through the array and print an invitation message to each person
for (const person of invitees) {
  console.log(`Dear ${person},\nYou are cordially invited to dinner. Please join us for an evening of great food and conversation.\nSincerely,\n[Your Name]`);
}


//15 changing guest list

// Create an array to store the names of people to invite
const invitees1: string[] = ["Albert Einstein", "Marie Curie", "Leonardo da Vinci"];

// Remove the guest who can't make it
const guestWhoCantMakeIt: string = "Marie Curie";
const indexOfGuestWhoCantMakeIt: number = invitees1.indexOf(guestWhoCantMakeIt);

if (indexOfGuestWhoCantMakeIt !== -1) {
  invitees1.splice(indexOfGuestWhoCantMakeIt, 1);
}

// Add a new guest
const newGuest: string = "Isaac Newton";
invitees1.push(newGuest);

// Loop through the updated array and print invitation messages
for (const person of invitees1) {
  console.log(`Dear ${person},\nYou are cordially invited to dinner. Please join us for an evening of great food and conversation.\nSincerely,\n[Your Name]`);
}


//16 more guests

// Create an array to store the names of people to invite
const invitees2: string[] = ["Albert Einstein", "Marie Curie", "Leonardo da Vinci"];

// Print the guest who can't make it
const guestWhoCantMakeIt1: string = "Marie Curie";
console.log(`${guestWhoCantMakeIt1} can't make it to the dinner.`);

// Remove the guest who can't make it
const indexOfGuestWhoCantMakeIt1: number = invitees.indexOf(guestWhoCantMakeIt1);
if (indexOfGuestWhoCantMakeIt1 !== -1) {
  invitees2.splice(indexOfGuestWhoCantMakeIt1, 1);
}

// Add a new guest
const newGuest1: string = "Isaac Newton";
invitees2.push(newGuest1);

// Invite additional guests
const additionalGuests: string[] = ["Nikola Tesla", "Ada Lovelace", "Charles Darwin"];

// Since you have a bigger table, invite three more guests
const moreGuests: string[] = ["Galileo Galilei", "Marie Curie's clone", "Grace Hopper"];
invitees2.push(...moreGuests);

// Print a message about the bigger dinner table
console.log("Good news! We found a bigger dinner table.");

// Loop through the updated array and print invitation messages
for (const person of invitees2) {
  console.log(`Dear ${person},\nYou are cordially invited to dinner. Please join us for an evening of great food and conversation.\nSincerely,\n[Your Name]`);
}


// 17  Shrinking guest list

// Create an array to store the names of people to invite
const invitees3: string[] = ["Albert Einstein", "Marie Curie", "Leonardo da Vinci", "Isaac Newton", "Nikola Tesla", "Ada Lovelace", "Charles Darwin", "Galileo Galilei", "Marie Curie's clone", "Grace Hopper"];

// Print the guest who can't make it
const guestWhoCantMakeIt2: string = "Marie Curie";
console.log(`${guestWhoCantMakeIt2} can't make it to the dinner.`);

// Remove the guest who can't make it
const indexOfGuestWhoCantMakeIt2: number = invitees.indexOf(guestWhoCantMakeIt2);
if (indexOfGuestWhoCantMakeIt !== -1) {
  invitees3.splice(indexOfGuestWhoCantMakeIt2, 1);
}

// Print a message about the bigger dinner table
console.log("Good news! We found a bigger dinner table.");

// Check if we can only invite two people
if (invitees3.length <= 2) {
  console.log("Unfortunately, we can only invite two people for dinner.");
}

// Remove guests until only two names remain
while (invitees3.length > 2) {
  const removedGuest = invitees3.pop();
  console.log(`Sorry ${removedGuest}, we can't invite you to dinner.`);
}

// Print invitations to the remaining two guests
for (const person of invitees3) {
  console.log(`Dear ${person},\nYou are still invited to dinner. Please join us for an evening of great food and conversation.\nSincerely,\n[Your Name]`);
}

// Remove the last two names from the list
invitees3.splice(0, invitees3.length);

// Print the empty list
console.log("Remaining guests:", invitees3);


// 18 seeing the world

// Create an array of places to visit
const placesToVisit: string[] = [
    "Paris, France",
    "Kyoto, Japan",
    "New York City, USA",
    "Machu Picchu, Peru",
    "Santorini, Greece"
  ];
  
  // Print the original order
  console.log("Original Order:");
  console.log(placesToVisit);
  
  // Print in alphabetical order (without modifying original)
  console.log("\nAlphabetical Order (without modifying original):");
  console.log([...placesToVisit].sort());
  
  // Verify original order is unchanged
  console.log("\nOriginal Order (still intact):");
  console.log(placesToVisit);
  
  // Print in reverse alphabetical order (without modifying original)
  console.log("\nReverse Alphabetical Order (without modifying original):");
  console.log([...placesToVisit].sort().reverse());
  
  // Verify original order is still unchanged
  console.log("\nOriginal Order (still intact):");
  console.log(placesToVisit);
  
  // Reverse the order of the list
  placesToVisit.reverse();
  console.log("\nReversed Order:");
  console.log(placesToVisit);
  
  // Reverse the order again to get back to the original order
  placesToVisit.reverse();
  console.log("\nBack to Original Order:");
  console.log(placesToVisit);
  
  // Sort in alphabetical order (modifies original)
  placesToVisit.sort();
  console.log("\nSorted in Alphabetical Order (modified original):");
  console.log(placesToVisit);
  
  // Sort in reverse alphabetical order (modifies original)
  placesToVisit.sort((a, b) => b.localeCompare(a));
  console.log("\nSorted in Reverse Alphabetical Order (modified original):");
  console.log(placesToVisit);
  

