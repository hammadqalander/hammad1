//  23  conditionaal tests


let age = 25;
let temperature = 80;
let isRaining = true;
let fruit = 'apple';
let city = 'New York';

// Test 1: Is age greater than 18?
console.log("Is age > 18? I predict True.");
console.log(age > 18); // True

// Test 2: Is temperature less than 90?
console.log("Is temperature < 90? I predict True.");
console.log(temperature < 90); // True

// Test 3: Is it raining?
console.log("Is it raining? I predict True.");
console.log(isRaining); // True

// Test 4: Is fruit equal to 'banana'?
console.log("Is fruit == 'banana'? I predict False.");
console.log(fruit == 'banana'); // False

// Test 5: Is city equal to 'New York'?
console.log("Is city == 'New York'? I predict True.");
console.log(city == 'New York'); // True

// Test 6: Is age less than or equal to 30?
console.log("Is age <= 30? I predict True.");
console.log(age <= 30); // True

// Test 7: Is temperature greater than or equal to 70?
console.log("Is temperature >= 70? I predict True.");
console.log(temperature >= 70); // True

// Test 8: Is it not raining?
console.log("Is it not raining? I predict False.");
console.log(!isRaining); // False

// Test 9: Is fruit not equal to 'apple'?
console.log("Is fruit != 'apple'? I predict False.");
console.log(fruit != 'apple'); // False

// Test 10: Is city not equal to 'Los Angeles'?
console.log("Is city != 'Los Angeles'? I predict True.");
console.log(city != 'Los Angeles'); // True


// 24 more conditional tests


let string1 = 'apple';
let string2 = 'banana';
let number1 = 42;
let number2 = 7;
let array = [1, 2, 3, 4, 5];

// Tests for equality and inequality with strings
console.log("Tests for equality and inequality with strings:");
console.log(string1 == 'apple'); // True
console.log(string1 != string2); // True
console.log(string1 == 'orange'); // False
console.log(string2 != 'banana'); // False

// Tests using the lower case function
console.log("\nTests using the lower case function:");
console.log(string1.toLowerCase() == 'apple'); // True
console.log(string2.toLowerCase() == 'Banana'.toLowerCase()); // True
console.log(string1.toLowerCase() == 'Banana'.toLowerCase()); // False

// Numerical tests involving equality and inequality, greater than and less than,
// greater than or equal to, and less than or equal to
console.log("\nNumerical tests involving numbers:");
console.log(number1 == 42); // True
console.log(number1 != number2); // True
console.log(number1 > number2); // True
console.log(number2 < number1); // True
console.log(number1 >= 42); // True
console.log(number2 <= 7); // True
console.log(number1 == 7); // False
console.log(number2 != 42); // False
console.log(number1 < number2); // False
console.log(number2 > 42); // False

// Tests using "and" and "or" operators
console.log("\nTests using 'and' and 'or' operators:");
console.log(true && true); // True
console.log(true || false); // True
console.log(false && true); // False
console.log(false || false); // False

// Test whether an item is in an array
console.log("\nTest whether an item is in an array:");
console.log(array.includes(3)); // True
console.log(array.includes(6)); // False

// Test whether an item is not in an array
console.log("\nTest whether an item is not in an array:");
console.log(!array.includes(6)); // True
console.log(!array.includes(2)); // False


//25  alien color


// Alien Colors #1
let alien_color = 'green'; // Change the color to 'yellow' or 'red' for different results

// Version 1: Passes the if test (alien_color is green)
if (alien_color === 'green') {
  console.log("Congratulations! You just earned 5 points for shooting the green alien.");
}

// Version 2: Fails the if test (alien_color is not green)
if (alien_color === 'red') {
  console.log("Congratulations! You just earned 5 points for shooting the green alien.");
}

//26 alien color 2

// Alien Colors #2
let alien_color2 = 'green'; // Change the color to 'yellow' or 'red' for different results

if (alien_color2 === 'green') {
  console.log("Congratulations! You just earned 5 points for shooting the green alien.");
} else {
  console.log("Congratulations! You just earned 10 points.");
}


